{% extends "@App/layout-cleanzone.html.twig" %}

{% block content %}

    <div class="page-head">
        <h2>Blank Page</h2>
        <ol class="breadcrumb">
            <li>
                <a href="#">Home</a>
            </li>
            <li>
                <a href="#">Category</a>
            </li>
            <li class="active">Sub Category</li>
        </ol>
    </div>
    <div class="cl-mcont">
        <div class="row">
            <div class="col-md-9">

                <div class="block-flat">
                    <div class="header">
                        <h2 class="title">Create a community</h2>
                    </div>
                    <div class="content">
                        <div class="share-widget clearfix">
                            {{ form_start(form) }}
                                {{ form_widget(form) }}

                                <button type="submit" class="btn btn-primary">Create</button>
                            {{ form_end(form) }}
                        </div>
                    </div>
                </div>

            </div>
            <div class="col-md-3"></div>
        </div>
    </div>

{% endblock content %}